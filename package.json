{
  "name": "react-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- next dev --turbopack",
    "build": "npx prisma generate && (npx prisma db push || echo \"[warn] Skipping prisma db push due to unexpected errors !\") && dotenv -e .env -- next build",
    "start": "dotenv -e .env -- next start",
    "lint": "next lint",
    "seed": "dotenv -e .env -- npx esrun ./app/_scripts/seed.ts",
    "hash": "dotenv -e .env -- npx esrun ./app/_scripts/generateHash.js"
  },
  "dependencies": {
    "@digitak/esrun": "^3.2.26",
    "@heroicons/react": "^2.1.5",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-table": "^8.20.5",
    "bcryptjs": "^2.4.3",
    "dotenv-cli": "^7.4.2",
    "jsonwebtoken": "^9.0.2",
    "ldapjs": "github:jiajunlee19/node-ldapjs",
    "mssql": "^11.0.1",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "prisma": "^5.22.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.5",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.5.4",
    "use-debounce": "^10.0.4",
    "uuid": "^11.0.2",
    "vercel": "^37.14.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "latest",
    "@types/bcryptjs": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/ldapjs": "^3.0.6",
    "@types/mssql": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/uuid": "latest",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "postcss": "latest",
    "tailwindcss": "latest",
    "typescript": "latest"
  }
}
